{
  "name": "client-side",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "browserify -t hbsfy index.js -g uglifyify > bundle.js",
    "dist": "mkdir -p dist && bash -c 'cp *.{js,html,css} dist'",
    "push": "gh-pages -d dist",
    "deploy": "npm run build && npm run dist && npm run push",

    "buildJade": "browserify -t jadeify index.js -g uglifyify > bundle.js",
    "deployJade": "npm run buildJade && npm run dist && npm run push",

    "buildAJAX": "browserify index.js -g uglifyify > bundle.js",
    "deployAJAX": "npm run buildAJAX && npm run dist && npm run push",

    "buildJqueryAJAX": "browserify index.js -g uglifyify > bundle.js",
    "deployJqueryAJAX": "npm run buildJqueryAJAX && npm run dist && npm run push",

    "buildWebpackJqueryAJAX": "webpack ./index.js -g uglifyify bundle.js",
    "deployWebpackJqueryAJAX": "npm run buildWebpackJqueryAJAX && npm run dist && npm run push"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "browserify": "^13.0.0",
    "budo": "^8.1.0",
    "gh-pages": "^0.11.0",
    "handlebars": "^4.0.5",
    "hbsfy": "^2.5.1",
    "hyperscript": "^1.4.7",
    "jade": "^1.11.0",
    "jadeify": "^4.6.0",
    "uglifyify": "^3.0.1",
    "webpack": "^1.12.14",
    "xhr": "^2.2.0"
  },
  "dependencies": {
    "jade": "^1.11.0"
  }
}
